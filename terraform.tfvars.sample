# values that override the defaults in variables.tf

facility = "ewr1"
webserver_count = 2
operating_system = "ubuntu_16_04"

#
# this overrides the default of using the keys in the home directory ~/.ssh/
# if you want to use a key in the local directoy, be sure to generate a keypair with:
# ssh-keygen -f ./id_rsa
#
public_key_filename = "./id_rsa.pub"
private_key_filename = "./id_rsa"
